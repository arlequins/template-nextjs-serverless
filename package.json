{
  "name": "template-nextjs-serverless",
  "version": "1.0.2",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npx serve@latest out",
    "lint": "next lint",
    "format": "prettier \"src/**/*.{js,ts,tsx,scss,json}\" --check",
